{
  "name": "openuv-adapter",
  "display_name": "OpenUV - Clear sky UV index",
  "version": "0.5.1",
  "description": "Get the current uv index from www.openuv.io",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-hellhake/openuv-adapter.git"
  },
  "author": "Tim Hellhake",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/tim-hellhake/openuv-adapter/issues"
  },
  "homepage": "https://github.com/tim-hellhake/openuv-adapter#readme",
  "files": [
    "LICENSE",
    "SHA256SUMS",
    "index.js",
    "openuv-adapter.js",
    "manifest.json"
  ],
  "moziot": {
    "api": {
      "min": 2,
      "max": 2
    },
    "plugin": true,
    "exec": "{nodeLoader} {path}",
    "config": {
      "maxUV": 14,
      "maxOzone": 500
    },
    "schema": {
      "type": "object",
      "required": [
        "apiKey",
        "latitude",
        "longitude",
        "maxUV",
        "maxOzone"
      ],
      "properties": {
        "apiKey": {
          "type": "string",
          "title": "The api key"
        },
        "latitude": {
          "type": "number",
          "title": "The latitude of your position"
        },
        "longitude": {
          "type": "number",
          "title": "The longitude of your position"
        },
        "maxUV": {
          "type": "number",
          "title": "The upper bound for the uv level indicator"
        },
        "maxOzone": {
          "type": "number",
          "title": "The upper bound for the ozone level indicator"
        }
      }
    }
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "babel-eslint": "^10.0.2"
  }
}
